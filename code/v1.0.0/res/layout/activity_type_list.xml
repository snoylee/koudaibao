<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent" >

    <ListView
        android:id="@+id/feedbackGroup"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/feedback_margin_left"
        android:layout_marginRight="@dimen/feedback_margin_right"
        android:background="@drawable/feedback_back"
        android:scrollbars="none"
        android:listSelector="@drawable/feedback_pop_click_selector"
        android:divider="@null"/>

</RelativeLayout>